<?xml version="1.0"?>
<classes>
    <class name="org.flymine.model.genomic.InteractionExperiment" extends="org.flymine.model.genomic.Experiment" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <attribute name="description" type="java.lang.String"/>
        <attribute name="hostOrganism" referenced-type="org.flymine.model.genomic.Organism" />
        <collection name="interactions" referenced-type="org.flymine.model.genomic.Interaction" ordered="true" reverse-reference="experiment"/>
    </class>
   <class name="org.flymine.model.genomic.InteractionTerm" extends="org.flymine.model.genomic.OntologyTerm" is-interface="true"/>

    <class name="org.flymine.model.genomic.Interaction" is-interface="true">
        <attribute name="role" type="java.lang.String"/>
        <attribute name="shortName" type="java.lang.String"/>
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="interactions" />
        <reference name="experiment" referenced-type="org.flymine.model.genomic.InteractionExperiment" reverse-reference="interactions"/>
        <collection name="interactingGenes" referenced-type="org.flymine.model.genomic.Gene" ordered="true" reverse-reference="interactions"/>
        <collection name="dataSets" referenced-type="org.flymine.model.genomic.DataSet"/>
    </class>
    <class name="org.flymine.model.genomic.Gene" is-interface="true">
        <collection name="interactions" referenced-type="org.flymine.model.genomic.Interaction" ordered="true" reverse-reference="gene"/>
    </class>
</classes>

