<?xml version="1.0"?>
<classes>
  <class name="org.flymine.model.genomic.RegulatoryRegion"
         extends="org.flymine.model.genomic.GenePart" is-interface="true">
    <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" 
               reverse-reference="regulatoryRegions"/>
  </class>
  <class name="org.flymine.model.genomic.Gene"
         extends="org.flymine.model.genomic.Region" 
         is-interface="true">
    <collection name="regulatoryRegions" 
                referenced-type="org.flymine.model.genomic.RegulatoryRegion" 
                ordered="true" reverse-reference="gene"/>
  </class>
</classes>
