<?xml version="1.0"?>

<classes>
  <class name="Stage" is-interface="true">
    <attribute name="identifier" type="java.lang.String"/>
    <attribute name="name" type="java.lang.String"/>
    <attribute name="start" type="java.lang.Double"/>
    <attribute name="end" type="java.lang.Double"/>
  </class>

  <class name="Gene" is-interface="true">
      <collection name="expressionResults" referenced-type="ExpressionResult" reverse-reference="gene"/>
  </class>

  <class name="ExpressionResult" is-interface="true">
    <attribute name="stageRange" type="java.lang.String"/>
    <attribute name="expressed" type="java.lang.Boolean"/>
    <reference name="dataSet" referenced-type="DataSet"/>
    <reference name="gene" referenced-type="Gene" reverse-reference="expressionResults"/>
    <collection name="images" referenced-type="Image"/>
    <collection name="stages" referenced-type="Stage"/>
    <collection name="anatomyTerms" referenced-type="AnatomyTerm"/>
  </class>

  <class name="Image" is-interface="true">
    <attribute name="url" type="java.lang.String"/>
  </class>

  <class name="AnatomyTerm" extends="OntologyTerm" is-interface="true" />

</classes>