<?xml version="1.0"?>
<classes>

  <class name="org.flymine.model.genomic.LociResult" is-interface="true">
    <attribute name="z" type="java.lang.Double"/>
    <attribute name="p" type="java.lang.Double"/>
    <attribute name="nStudies" type="java.lang.Integer"/>
    <attribute name="nSubjects" type="java.lang.Integer"/>
    <attribute name="metaBeta" type="java.lang.Double"/>
    <attribute name="metaSe" type="java.lang.Double"/>
    <attribute name="location" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
    <attribute name="details" type="java.lang.String"/>
    <attribute name="mousePhenotype" type="java.lang.String"/>
    <attribute name="humanPhenotype" type="java.lang.String"/>
    <attribute name="geneName" type="java.lang.String"/>
    <reference name="snp" referenced-type="org.flymine.model.genomic.SNP" reverse-rference="lociResults"/>
    <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="lociResult"/>
</class>

  <class name="org.flymine.model.genomic.Gene" is-interface="true">
    <reference name="lociResult" referenced-type="org.flymine.model.genomic.LociResult" reverse-reference="genes"/>   
  </class>

  <class name="org.flymine.model.genomic.SNP" is-interface="true">
    <attribute name="snp" type="java.lang.String"/>
    <attribute name="allele1" type="java.lang.String"/>
    <attribute name="allele2" type="java.lang.String"/>
    <reference name="location" referenced-type="org.flymine.model.genomic.Location"/>
    <collection name="lociResults" referenced-type="org.flymine.model.genomic.LociResult" reverse-reference="snp"/>
  </class>


  <class name="org.flymine.model.genomic.Location" is-interface="true">
    <reference name="chromosome" referenced-type="org.flymine.model.genomic.Chromosome"/>
  </class>

  <class name="org.flymine.model.genomic.Chromosome" is-interface="true">
    <reference name="location" referenced-type="org.flymine.model.genomic.Location"/>
  </class>

</classes>
