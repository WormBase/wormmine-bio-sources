<?xml version="1.0"?>
<classes>
   <class name="org.flymine.model.genomic.Gene" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
     <attribute name="featureType" type="java.lang.String"/>
     <attribute name="curated" type="java.lang.String"/>
     <reference name="chromosome" referenced-type="org.flymine.model.genomic.Chromosome"/>
     <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript" ordered="true" reverse-reference="gene" />
   </class>

  <class name="org.flymine.model.genomic.Protein" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
    <attribute name="md5checksum" type="java.lang.String"/>
    <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript" reverse-reference="protein" />
    <collection name="CDSs" referenced-type="org.flymine.model.genomic.CDS" reverse-reference="protein" />
  </class>

   <class name="org.flymine.model.genomic.Transcript" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
     <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="transcripts" />
     <reference name="sequence" referenced-type="org.flymine.model.genomic.Sequence" />
     <reference name="protein" referenced-type="org.flymine.model.genomic.Protein" reverse-reference="transcripts" />
     <collection name="exons" referenced-type="org.flymine.model.genomic.Exon" ordered="true" reverse-reference="transcripts" />     
   </class>

   <class name="org.flymine.model.genomic.Exon" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
     <collection name="transcripts" referenced-type="org.flymine.model.genomic.Transcript" reverse-reference="exons" />
   </class>

   <class name="org.flymine.model.genomic.CDS" extends="org.flymine.model.genomic.GenePart" is-interface="true">
     <reference name="protein" referenced-type="org.flymine.model.genomic.Protein" reverse-reference="CDSs" />
     <reference name="sequence" referenced-type="org.flymine.model.genomic.Sequence" />
     <reference name="MRNA" referenced-type="org.flymine.model.genomic.MRNA" reverse-reference="CDSs" />
   </class>

  <class name="org.flymine.model.genomic.MRNA" extends="org.flymine.model.genomic.ProcessedTranscript" is-interface="true">
    <reference name="protein" referenced-type="org.flymine.model.genomic.Protein" reverse-reference="transcripts" />
    <collection name="CDSs" referenced-type="org.flymine.model.genomic.CDS" reverse-reference="MRNA"/>    
  </class>

</classes>
