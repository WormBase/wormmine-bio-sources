<?xml version="1.0"?>
<classes>
  <class name="org.flymine.model.genomic.RNAiScreen"
         extends="org.flymine.model.genomic.Experiment" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="cellLine" type="java.lang.String"/>
    <attribute name="phenotypeDescription" type="java.lang.String"/>
    <reference name="organism" referenced-type="org.flymine.model.genomic.Organism"/> 
    <collection name="screenHits"
                referenced-type="org.flymine.model.genomic.ScreenHit" reverse-reference="screen"/> 
  </class>
  <class name="org.flymine.model.genomic.ScreenHit" 
         is-interface="true">
    <attribute name="strength" type="java.lang.Integer"/>
    <attribute name="phenotype" type="java.lang.String"/>
    <attribute name="numOffTargets" type="java.lang.Integer"/>
    <attribute name="maxOffTargetOverlaps" type="java.lang.Integer"/>
    <reference name="screen" referenced-type="org.flymine.model.genomic.RNAiScreen" 
               reverse-reference="screenHits"/>
    <reference name="gene" referenced-type="org.flymine.model.genomic.Gene"/>
    <reference name="amplicon"
               referenced-type="org.flymine.model.genomic.Amplicon" 
               reverse-reference="screenHits"/>
  </class>
  <class name="org.flymine.model.genomic.Amplicon" 
         extends="org.flymine.model.genomic.LocatedSequenceFeature"
         is-interface="true">
    <collection name="screenHits" referenced-type="org.flymine.model.genomic.ScreenHit"/>
  </class>
</classes>
