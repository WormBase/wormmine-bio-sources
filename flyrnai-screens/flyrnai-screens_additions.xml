<?xml version="1.0"?>
<classes>
  <class name="org.flymine.model.genomic.RNAiScreen"
         extends="org.flymine.model.genomic.Experiment" is-interface="true">
    <attribute name="name" type="java.lang.String"/>
    <attribute name="cellLine" type="java.lang.String"/>
    <attribute name="analysisDescription" type="java.lang.String"/>
    <reference name="organism" referenced-type="org.flymine.model.genomic.Organism"/> 
  </class>
  <class name="org.flymine.model.genomic.RNAiScreenHit" extends="org.flymine.model.genomic.RNAiResult"
         is-interface="true">
    <attribute name="strength" type="java.lang.Integer"/>
    <attribute name="hasPredictedOffTargetEffect" type="java.lang.Boolean"/>
    <attribute name="result" type="java.lang.String"/>
    <attribute name="carRepeats" type="java.lang.String"/>
    <attribute name="numOffTargets" type="java.lang.Integer"/>
    <reference name="amplicon"
               referenced-type="org.flymine.model.genomic.Amplicon" 
               reverse-reference="screenHits"/>
  </class>
  <class name="org.flymine.model.genomic.Amplicon" 
         extends="org.flymine.model.genomic.LocatedSequenceFeature"
         is-interface="true">
    <collection name="screenHits" referenced-type="org.flymine.model.genomic.RNAiScreenHit"/>
  </class>
  <class name="org.flymine.model.genomic.RNAiResult" extends="org.flymine.model.genomic.ExperimentalResult" is-interface="true">
    <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="rnaiResults"/>
  </class>
<class name="org.flymine.model.genomic.Gene" is-interface="true">
  <collection name="rnaiResults" referenced-type="org.flymine.model.genomic.RNAiResult" reverse-reference="gene"/>
</class>
</classes>
