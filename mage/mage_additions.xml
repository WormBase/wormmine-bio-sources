<?xml version="1.0"?>
<classes>
<class name="org.flymine.model.genomic.MicroArrayAssay" is-interface="true">
  <attribute name="name" type="java.lang.String"/>
  <attribute name="description" type="java.lang.String"/>
  <attribute name="sample1" type="java.lang.String"/>
  <attribute name="sample2" type="java.lang.String"/>
  <attribute name="displayOrder" type="java.lang.Integer"/>
  <reference name="experiment" referenced-type="org.flymine.model.genomic.MicroArrayExperiment" reverse-reference="assays"/> 
  <collection name="results" referenced-type="org.flymine.model.genomic.MicroArrayResult"  reverse-reference="assays"/>
  <collection name="samples" referenced-type="org.flymine.model.genomic.Sample" reverse-reference="assays"/>
</class>
<class name="org.flymine.model.genomic.ProbeSet" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
  <collection name="results" referenced-type="org.flymine.model.genomic.MicroArrayResult" reverse-reference="material"/>
</class>
<class name="org.flymine.model.genomic.SampleCharacteristic" is-interface="true">
  <attribute name="type" type="java.lang.String"/>
  <attribute name="value" type="java.lang.String"/>
  <reference name="ontologyTerm" referenced-type="org.flymine.model.genomic.OntologyTerm"/>
</class>
<class name="org.flymine.model.genomic.Treatment" is-interface="true">
  <attribute name="action" type="java.lang.String"/>
  <collection name="protocols" referenced-type="org.flymine.model.genomic.Protocol"/>
  <collection name="parameters" referenced-type="org.flymine.model.genomic.TreatmentParameter" reverse-reference="treatment"/>
</class>
<class name="org.flymine.model.genomic.TreatmentParameter" is-interface="true">
  <attribute name="type" type="java.lang.String"/>
  <attribute name="value" type="java.lang.String"/>
  <attribute name="units" type="java.lang.String"/>
  <reference name="treatment" referenced-type="org.flymine.model.genomic.Treatment" reverse-reference="parameters"/>
</class>
<class name="org.flymine.model.genomic.Protocol" is-interface="true">
  <attribute name="name" type="java.lang.String"/>
  <attribute name="description" type="java.lang.String"/>
</class>
<class name="org.flymine.model.genomic.MicroArrayExperiment" extends="org.flymine.model.genomic.Experiment" is-interface="true">
  <attribute name="identifier" type="java.lang.String"/>
  <attribute name="description" type="java.lang.String"/>
  <attribute name="name" type="java.lang.String"/>
  <collection name="assays" referenced-type="org.flymine.model.genomic.MicroArrayAssay" reverse-reference="experiment"/>
  <collection name="results" referenced-type="org.flymine.model.genomic.MicroArrayResult" reverse-reference="experiment"/>
</class>
<class name="org.flymine.model.genomic.Reporter" is-interface="true">
  <attribute name="isControl" type="java.lang.Boolean"/>
  <attribute name="failType" type="java.lang.String"/>
  <attribute name="controlType" type="java.lang.String"/>
  <reference name="material" referenced-type="org.flymine.model.genomic.BioEntity"/>
  <collection name="results" referenced-type="org.flymine.model.genomic.MicroArrayResult" reverse-reference="reporters"/>
</class>

<class name="org.flymine.model.genomic.CDNAClone" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
  <collection name="results" referenced-type="org.flymine.model.genomic.MicroArrayResult"/>
</class>
<class name="org.flymine.model.genomic.NuclearDNA" extends="org.flymine.model.genomic.BioEntity" is-interface="true"/>
<class name="org.flymine.model.genomic.Vector" extends="org.flymine.model.genomic.BioEntity" is-interface="true"/>
<class name="org.flymine.model.genomic.Sample" extends="org.flymine.model.genomic.BioEntity" is-interface="true">
  <attribute name="materialType" type="java.lang.String"/>
  <attribute name="name" type="java.lang.String"/>
  <attribute name="description" type="java.lang.String"/>
  <attribute name="primaryCharacteristic" type="java.lang.String"/>
  <attribute name="primaryCharacteristicType" type="java.lang.String"/>
  <collection name="assays" referenced-type="org.flymine.model.genomic.MicroArrayAssay" reverse-reference="samples"/>
  <collection name="characteristics" referenced-type="org.flymine.model.genomic.SampleCharacteristic" ordered="true"/>
  <collection name="treatments" referenced-type="org.flymine.model.genomic.Treatment" ordered="true"/>
</class>
<class name="org.flymine.model.genomic.MicroArrayResult" extends="org.flymine.model.genomic.ExperimentalResult" is-interface="true">
  <attribute name="scale" type="java.lang.String"/>
  <attribute name="type" type="java.lang.String"/>
  <attribute name="isControl" type="java.lang.Boolean"/>
  <attribute name="value" type="java.lang.Float"/>
  <reference name="experiment" referenced-type="org.flymine.model.genomic.MicroArrayExperiment" reverse-reference="results"/>
  <reference name="material" referenced-type="org.flymine.model.genomic.ProbeSet" reverse-reference="results"/>
  <collection name="assays" referenced-type="org.flymine.model.genomic.MicroArrayAssay" reverse-reference="results"/>
  <collection name="reporters" referenced-type="org.flymine.model.genomic.Reporter" reverse-reference="results"/>
  <collection name="genes" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="microArrayResults"/>
  <collection name="samples" referenced-type="org.flymine.model.genomic.Sample" ordered="true"/>
</class>
<class name="org.flymine.model.genomic.Gene" is-interface="true">
  <collection name="microArrayResults" referenced-type="org.flymine.model.genomic.MicroArrayResult" reverse-reference="genes"/>
</class>
