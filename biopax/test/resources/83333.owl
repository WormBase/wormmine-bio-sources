    <bp:TITLE rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Glycogen metabolism and glycolytic enzymes</bp:TITLE>
    <bp:AUTHORS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Villar-Palasi, C</bp:AUTHORS>
    <bp:AUTHORS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Larner, J</bp:AUTHORS>
