  <bp:evidence rdf:ID="evidence62">
    <bp:XREF rdf:resource="#publicationXref114" />
    <bp:XREF rdf:resource="#publicationXref115" />
    <bp:XREF rdf:resource="#publicationXref116" />
    <bp:XREF rdf:resource="#publicationXref117" />
