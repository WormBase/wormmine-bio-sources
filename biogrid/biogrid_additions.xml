<?xml version="1.0"?>
<classes>
    <class name="org.flymine.model.genomic.GeneInteractionExperiment" extends="org.flymine.model.genomic.Experiment" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <attribute name="description" type="java.lang.String"/>        
        <collection name="interactions" referenced-type="org.flymine.model.genomic.GeneInteraction" ordered="true" reverse-reference="experiment"/>
    </class>

    <class name="org.flymine.model.genomic.GeneInteraction" is-interface="true">
        <attribute name="type" type="java.lang.String"/>
        <attribute name="shortName" type="java.lang.String"/>                
        <reference name="gene" referenced-type="org.flymine.model.genomic.Gene" reverse-reference="geneInteractions" />
        <reference name="experiment" referenced-type="org.flymine.model.genomic.GeneInteractionExperiment" reverse-reference="interactions"/>
        <collection name="interactingGenes" referenced-type="org.flymine.model.genomic.Gene" ordered="true" reverse-reference="geneInteractions"/>
        <collection name="evidence" referenced-type="org.flymine.model.genomic.Evidence"/>
    </class>

    <class name="org.flymine.model.genomic.Gene" is-interface="true">       
        <collection name="geneInteractions" referenced-type="org.flymine.model.genomic.GeneInteraction" ordered="true" reverse-reference="gene"/>
    </class>
</classes>
	