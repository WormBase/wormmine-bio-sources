<?xml version="1.0"?>
<classes>
  <class name="PhenotypeAnnotation" extends="Annotation" is-interface="true">
    <attribute name="description" type="java.lang.String"/>
    <attribute name="annotationType" type="java.lang.String"/>
    <reference name="allele" referenced-type="Allele" 
               reverse-reference="phenotypeAnnotations"/>
    <!--
      - removed as the "with" clauses from FlyBase have errors - see ticket #889
    <collection name="with" referenced-type="Allele"/>
    -->
    <reference name="anatomyTerm" referenced-type="AnatomyTerm"/>
    <reference name="developmentTerm" referenced-type="DevelopmentTerm"/>
    <collection name="cvTerms" referenced-type="CVTerm"/>
    <collection name="publications" referenced-type="Publication"/>
    <collection name="dataSets" referenced-type="DataSet"/>
  </class>
  <class name="Allele" 
         extends="BioEntity" is-interface="true">
    <attribute name="alleleClass" type="java.lang.String"/>
    <collection name="mutagens"
                referenced-type="Mutagen"
                reverse-reference="alleles"/>
    <collection name="phenotypeAnnotations"
                referenced-type="PhenotypeAnnotation" 
                reverse-reference="allele"/>
  </class>
  <!-- mv from biogrid/biogrid_additions.xml  -->
    <class name="InteractionExperiment" extends="Experiment" is-interface="true">
      <attribute name="name" type="java.lang.String"/>
<!--       <attribute name="publication" type="Publication"/> -->
<!--       <attribute name="description" type="java.lang.String"/> -->
<!--       <attribute name="hostOrganism" type="java.lang.String" /> -->
      <collection name="interactions" referenced-type="Interaction" ordered="true" reverse-reference="experiment"/>
    </class>
    
         <class name="Interaction" is-interface="true">
           <attribute name="shortName" type="java.lang.String"/>
           <attribute name="interactionType" type="java.lang.String"/>
           <reference name="gene" referenced-type="Gene" reverse-reference="interactions" />
           <reference name="experiment" referenced-type="InteractionExperiment" reverse-reference="interactions"/>
           <collection name="interactingGenes" referenced-type="Gene" ordered="true" reverse-reference="interactions"/>
<!--            <collection name="dataSets" referenced-type="DataSet"/> -->
         </class>
         
         <class name="Gene" is-interface="true">
           <attribute name="featureType" type="java.lang.String"/>
           <collection name="interactions" referenced-type="Interaction" ordered="true" reverse-reference="gene"/>
         </class>
         

  <class name="Mutagen" is-interface="true">
    <attribute name="description" type="java.lang.String"/>
    <collection name="alleles"
                referenced-type="Allele"
                reverse-reference="mutagens"/>
  </class>

  <class name="AnatomyTerm" extends="OntologyTerm" is-interface="true">
    
  </class>
  <class name="DevelopmentTerm" extends="OntologyTerm" is-interface="true">
    
  </class>
  <class name="CVTerm" extends="OntologyTerm" is-interface="true">
    
  </class>

<!--  <class name="Gene" is-interface="true">
    <attribute name="featureType" type="java.lang.String"/>
  </class>-->

  <class name="ChromosomeStructureVariation" 
         extends="LocatedSequenceFeature"
         is-interface="true">
    <collection name="featureTerms"
                referenced-type="SequenceOntologyTerm"/>
  </class>
</classes>
