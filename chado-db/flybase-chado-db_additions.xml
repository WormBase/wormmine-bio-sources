<?xml version="1.0"?>
<classes>
  <class name="org.flymine.model.genomic.PhenotypeAnnotation" extends="org.flymine.model.genomic.Annotation" is-interface="true">
    <attribute name="description" type="java.lang.String"/>
    <attribute name="annotationType" type="java.lang.String"/>
    <reference name="allele" referenced-type="org.flymine.model.genomic.Allele" 
               reverse-reference="phenotypeAnnotations"/>
    <!--
      - removed as the "with" clauses from FlyBase have errors - see ticket #889
    <collection name="with" referenced-type="org.flymine.model.genomic.Allele"/>
    -->
    <reference name="anatomyTerm" referenced-type="org.flymine.model.genomic.AnatomyTerm"/>
    <reference name="developmentTerm" referenced-type="org.flymine.model.genomic.DevelopmentTerm"/>
    <collection name="cvTerms" referenced-type="org.flymine.model.genomic.CVTerm"/>
    <collection name="publications" referenced-type="org.flymine.model.genomic.Publication"/>
    <collection name="dataSets" referenced-type="org.flymine.model.genomic.DataSet"/>
  </class>
  <class name="org.flymine.model.genomic.Allele" 
         extends="org.flymine.model.genomic.BioEntity" is-interface="true">
    <attribute name="alleleClass" type="java.lang.String"/>
    <collection name="mutagens"
                referenced-type="org.flymine.model.genomic.Mutagen"
                reverse-reference="alleles"/>
    <collection name="phenotypeAnnotations"
                referenced-type="org.flymine.model.genomic.PhenotypeAnnotation" 
                reverse-reference="allele"/>
  </class>
  <class name="org.flymine.model.genomic.Mutagen" is-interface="true">
    <attribute name="description" type="java.lang.String"/>
    <collection name="alleles"
                referenced-type="org.flymine.model.genomic.Allele"
                reverse-reference="mutagens"/>
  </class>

  <class name="org.flymine.model.genomic.AnatomyTerm" extends="org.flymine.model.genomic.OntologyTerm" is-interface="true">
    
  </class>
  <class name="org.flymine.model.genomic.DevelopmentTerm" extends="org.flymine.model.genomic.OntologyTerm" is-interface="true">
    
  </class>
  <class name="org.flymine.model.genomic.CVTerm" extends="org.flymine.model.genomic.OntologyTerm" is-interface="true">
    
  </class>

</classes>
