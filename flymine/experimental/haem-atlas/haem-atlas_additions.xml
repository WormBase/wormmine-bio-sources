<?xml version="1.0"?>
<classes>

  <class name="HaemAtlasProbeSet" is-interface="true">
    <attribute name="illuId" type="java.lang.String"/>
    <reference name="gene" referenced-type="Gene" reverse-reference="probeSets"/>
    <collection name="haemAtlasResults" referenced-type="HaemAtlasResult" reverse-reference="probeSet"/>
  </class>

  <class name="Gene" is-interface="true">
    <collection name="probeSets" referenced-type="HaemAtlasProbeSet" reverse-reference="gene"/>   
  </class>
  
  <class name="HaemAtlasResult" is-interface="true">
    <attribute name="group" type="java.lang.String"/>
    <attribute name="averageIntensity" type="java.lang.Double"/>
    <attribute name="sample" type="java.lang.String"/>
    <attribute name="detectionProbabilities" type="java.lang.Double"/>
    <attribute name="sampleName" type="java.lang.String"/>
    <reference name="probeSet" referenced-type="HaemAtlasProbeSet" reverse-reference="haemAtlasResults"/> 
  </class>

</classes>
