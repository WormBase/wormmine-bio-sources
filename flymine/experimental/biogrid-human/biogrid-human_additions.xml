<?xml version="1.0"?>
<classes>
    <class name="GeneticInteractionExperiment" extends="Experiment" is-interface="true">
        <attribute name="name" type="java.lang.String"/>
        <attribute name="description" type="java.lang.String"/>
        <collection name="interactions" referenced-type="GeneticInteraction" reverse-reference="experiment"/>
    </class>

    <class name="GeneticInteraction" is-interface="true">
        <attribute name="type" type="java.lang.String"/>
        <attribute name="shortName" type="java.lang.String"/>
        <attribute name="geneRole" type="java.lang.String"/>
        <reference name="gene" referenced-type="Gene" reverse-reference="geneticInteractions" />
        <reference name="experiment" referenced-type="GeneticInteractionExperiment" reverse-reference="interactions"/>
        <collection name="interactingGenes" referenced-type="Gene" reverse-reference="geneticInteractions"/>
        <collection name="evidence" referenced-type="Evidence"/>
    </class>

    <class name="Gene" is-interface="true">
        <collection name="geneticInteractions" referenced-type="GeneticInteraction" reverse-reference="gene"/>
    </class>
</classes>

