<?xml version="1.0"?>
<classes>

	<class name="Publication" is-interface="true">
		<attribute name="refLine" type="java.lang.String"/>
		<reference name="prideExperiment" referenced-type="PrideExperiment"/>
	</class>

	<class name="MedSubject" is-interface="true">
		<attribute name="name" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.String"/>
	</class>

	<class name="PrideDisease" is-interface="true">
		<attribute name="name" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.String"/>
	</class>

	<class name="Tissue" is-interface="true">
		<attribute name="name" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.String"/>
	</class>

	<class name="PSIMod" is-interface="true">
		<attribute name="name" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.String"/>
	</class>

	<class name="CellType" is-interface="true">
		<attribute name="name" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.String"/>
	</class>

	<class name="ProteinIdentification" is-interface="true">
		<attribute name="score" type="java.lang.Float"/>
		<attribute name="threshold" type="java.lang.Float"/>
		<attribute name="searchEngine" type="java.lang.String"/>
		<attribute name="spliceIsoform" type="java.lang.String"/>
		<attribute name="spectrumReference" type="java.lang.String"/>
		<attribute name="sequenceCoverage" type="java.lang.Float"/>
		<attribute name="molecularWeight" type="java.lang.Float"/>
		<attribute name="pI" type="java.lang.Float"/>
		<attribute name="gelLink" type="java.lang.String"/>
		<attribute name="gelXCoordinate" type="java.lang.Float"/>
		<attribute name="gelYCoordinate" type="java.lang.Float"/>
		<collection name="peptides" referenced-type="Peptide" reverse-reference="proteinIdentification"/>
		<reference name="protein" referenced-type="Protein" reverse-reference="proteinIdentifications"/>
		<reference name="prideExperiment" referenced-type="PrideExperiment" reverse-reference="proteinIdentifications"/>
	</class>


	<class name="Protein" is-interface="true">
                <collection name="peptides" referenced-type="Peptide" reverse-reference="proteins"/>
		<collection name="proteinIdentifications" referenced-type="ProteinIdentification" reverse-reference="protein"/>
	</class>

	<class name="Peptide" is-interface="true">
		<attribute name="sequence" type="java.lang.String"/>
		<attribute name="start" type="java.lang.Integer"/>
		<attribute name="end" type="java.lang.Integer"/>
		<attribute name="spectrumReference" type="java.lang.Float"/>
		<collection name="proteins" referenced-type="Protein" reverse-reference="peptides"/>
		<reference name="proteinIdentification" referenced-type="ProteinIdentification" reverse-reference="peptides"/>
		<collection name="peptideModifications" referenced-type="PeptideModification"/>
	</class>

	<class name="PeptideModification" is-interface="true">
		<attribute name="location" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.String"/>
		<attribute name="modDB" type="java.lang.String"/>
		<attribute name="modDBVersion" type="java.lang.String"/>
		<attribute name="modMonoDelta" type="java.lang.String"/>
		<attribute name="modAvgDelta" type="java.lang.String"/>
		<reference name="peptide" referenced-type="Peptide"/>
	</class>


  	<class name="PrideProject" is-interface="true">
		<attribute name="title" type="java.lang.String"/>
		<collection name="prideExperiments" referenced-type="PrideExperiment" reverse-reference="prideProject"/>	
	</class>	

	<class name="PrideExperiment" is-interface="true">
		<attribute name="title" type="java.lang.String"/>
		<attribute name="shortLabel" type="java.lang.String"/>
		<attribute name="accessionId" type="java.lang.Integer"/>
		<reference name="prideProject" referenced-type="PrideProject" reverse-reference="prideExperiments"/>
		<reference name="organism" referenced-type="Organism"/>
		<collection name="publications" referenced-type="Publication"/>
		<reference name="goTerm" referenced-type="OntologyTerm"/>
		<reference name="psiMod" referenced-type="PSIMod"/>
		<reference name="medSubject" referenced-type="MedSubject"/>
		<reference name="cellType" referenced-type="CellType"/>
		<reference name="tissue" referenced-type="Tissue"/>
		<reference name="disease" referenced-type="PrideDisease"/>
		<collection name="proteinIdentifications" referenced-type="ProteinIdentification" reverse-reference="prideExperiment"/>
	</class>


</classes>
